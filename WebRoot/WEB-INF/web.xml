<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
xmlns="http://java.sun.com/xml/ns/javaee" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"> 
	<display-name>ZWYSBBS</display-name>
	<welcome-file-list>
	<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet> 
		<servlet-name>login</servlet-name> 
		<servlet-class>com.wzbcbbs.servlet.UserloginServlet</servlet-class> 
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name> 
		<url-pattern>/UserloginServlet</url-pattern>
	</servlet-mapping>
	
	<servlet> 
		<servlet-name>dome</servlet-name> 
		<servlet-class>com.wzbcbbs.servlet.DomeServlet</servlet-class> 
	</servlet>
	<servlet-mapping>
		<servlet-name>dome</servlet-name> 
		<url-pattern>/DomeServlet</url-pattern>
	</servlet-mapping>
	
	<servlet> 
		<servlet-name>register</servlet-name> 
		<servlet-class>com.wzbcbbs.servlet.doRegisterservlet</servlet-class> 
	</servlet>
	<servlet-mapping>
		<servlet-name>register</servlet-name> 
		<url-pattern>/doRegisterservlet</url-pattern>
	</servlet-mapping>
</web-app> 